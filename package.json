{
  "name": "playwright-results-parser",
  "version": "1.0.2",
  "description": "Core building block for Playwright test analysis tools - provides foundational parsing, normalization, and transformation APIs that other packages can build upon.",
  "keywords": [
    "automation",
    "playwright",
    "parser",
    "reporting",
    "testing",
    "test-results",
    "test-automation"
  ],
  "homepage": "https://github.com/idanfishman/playwright-results-parser#readme",
  "bugs": {
    "url": "https://github.com/idanfishman/playwright-results-parser/issues"
  },
  "license": "MIT",
  "author": "Idan Fishman",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/idanfishman/playwright-results-parser.git"
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "esbuild": "0.25.9",
    "eslint": "9.33.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
